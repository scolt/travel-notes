{
    "name": "emp",
    "version": "1.0.0",
    "description": "EPAM Mentoring Project",
    "main": "app.js",
    "scripts": {
        "start": "npm-run-all -p node webpack",
        "node": "node app",
        "webpack": "webpack",
        "test": "eslint -c .eslintrc.js --ext .js --ext .jsx client/js",
        "postinstall": "if test \"$NODE_ENV\" = \"production\" ; then webpack --progress --colors ; fi",
        "selenium-server": "node node_modules/selenium-standalone/bin/selenium-standalone start",
        "e2e-setup": "node node_modules/selenium-standalone/bin/selenium-standalone install",
        "e2e": "node_modules/.bin/wdio tests/e2e/wdio.conf.js",
        "e2e-full": "npm run clean-report & npm run e2e & npm run generate-report & npm run open-report",
        "generate-report": "node node_modules/allure-commandline/bin/allure generate ./tests/e2e/.reports/allure/xml/ -o ./tests/e2e/.reports/allure/html",
        "open-report": "node node_modules/allure-commandline/bin/allure report open --report-dir ./tests/e2e/.reports/allure/html",
        "clean-report": "node node_modules/allure-commandline/bin/allure report clean --report-dir ./tests/e2e/.reports/allure/xml & node node_modules/allure-commandline/bin/allure report clean --report-dir ./tests/e2e/.reports/allure/html"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh:/git@github.com:scolt/travel-notes.git"
    },
