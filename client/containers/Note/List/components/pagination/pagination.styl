.pagination
    display block
    list-style none
    padding 0
    margin 10px 0
    text-align center

    li
        display inline-block
        width 25px
        height 25px
        text-align center
        line-height 1.5
        border 1px solid #00bcd4
        margin-right 10px
        cursor pointer

        &.active
            color #fff
            background #00bcd4

